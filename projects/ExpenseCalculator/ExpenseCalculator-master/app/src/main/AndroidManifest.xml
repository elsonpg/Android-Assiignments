<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="expenser.elson.com.expensecalculator" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        tools:replace="icon, label">
        <activity
            android:name="com.elson.expenser.activities.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.elson.expenser.activities.ActivityAddIncome" android:label="Income Categories"/>
        <activity android:name="com.elson.expenser.activities.AddIncomeForm" android:label="Add Income"/>
    </application>

</manifest>
